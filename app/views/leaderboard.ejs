<!doctype html>
<html lang="en" ng-app="Capriccioso">

<head>
  <% include ../views/partials/head %>
</head>

<body ng-controller="LeaderController as leaderCtrl">
  <header>
    <% include ../views/partials/header %>
  </header>

  <main>

    <table>
      <thead>
        <tr>
          <th>Username</th>
          <th>Score</th>
        </tr>
      </thead>
      <tbody>
        <tr ng-repeat="user in leaderCtrl.users">
          <td ng-bind="user.username"></td>
            <td ng-bind="user.score"></td>
</tbody>
  </main>
</body>

</html>
